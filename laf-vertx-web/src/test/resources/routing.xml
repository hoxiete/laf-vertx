<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<vertx connection="" exception="">
    <routes>
        <route name="default" route="true" type="POST"
               consumes="application/json"
               produces="application/json"
               handlers="{} render"
               errors="error"/>
        <route path="/hello" inherit="default" produces="text/plain" handlers="helloWorld"/>
        <route path="/hello1/test" inherit="default" produces="text/plain" handlers="helloWorld#test"/>
    </routes>
    <messages>
        <message path="profile_clear_event" handlers="test"/>
    </messages>
</vertx>